{
  "name": "bs-code",
  "description": "bs for code",
  "version": "1.3.0",
  "activationEvents": [
    "onCommand:bsCode.createAndOpenB",
    "onCommand:bsCode.insertMarkdownAnchors",
    "onCommand:bsCode.openPairFile"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "bsCode.createAndOpenB",
        "title": "bs-code: Create And Open B"
      },
      {
        "command": "bsCode.insertMarkdownAnchors",
        "title": "bs-code: Insert Markdown Anchors"
      },
      {
        "command": "bsCode.openPairFile",
        "title": "bs-code: Open Pair File"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "bs-code configuration",
      "properties": {
        "bsCode.rootDirectory": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "root directory."
        }
      }
    }
  },
  "dependencies": {
    "expand-markdown-anchors": "https://github.com/bouzuya/expand-markdown-anchors/archive/0.2.0.tar.gz",
    "fs-extra": "^1.0.0",
    "time-keeper": "^2.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^0.0.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "@types/proxyquire": "1.3.27",
    "@types/sinon": "1.16.32",
    "mocha": "^2.3.3",
    "proxyquire": "1.7.10",
    "sinon": "1.17.6",
    "typescript": "^2.0.3",
    "vsce": "^1.16.0",
    "vscode": "^1.0.0"
  },
  "displayName": "bs-code",
  "engines": {
    "vscode": "^1.5.0"
  },
  "main": "./out/src/extension",
  "publisher": "bouzuya",
  "scripts": {
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "tsc -p ./"
  }
}
